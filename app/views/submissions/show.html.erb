<p id="notice"><%= notice %></p>

<p>
  <b>User:</b>
  <%= @submission.user_id %>
</p>

<p>
  <b>Name:</b>
  <%= @submission.name %>
</p>

<p>
  <b>Description:</b>
  <%= @submission.description %>
</p>

<p>
  <b>Challenge:</b>
  <%= @submission.challenge %>
</p>

<% if current_user.id == @submission.user_id %>
	<%= link_to 'Edit', edit_submission_path(@submission) %> |
<% end %>
<a href="<%= @submission.github_url %>">View on Github</a>
